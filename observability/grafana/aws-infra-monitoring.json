{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "EC2 CPU Utilization (%)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 - (avg by(instance)(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
          "legendFormat": "{{instance}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "EC2 Memory Usage (%)",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "((node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes) * 100",
          "legendFormat": "memory_used_pct",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 3,
      "type": "gauge",
      "title": "EC2 Disk Usage (%)",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (node_filesystem_free_bytes{fstype!~\"tmpfs|overlay\"} / node_filesystem_size_bytes{fstype!~\"tmpfs|overlay\"})) * 100",
          "legendFormat": "disk_used_pct",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "RDS CPU Utilization (%)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "aws_rds_cpuutilization_average{dimension_DBInstanceIdentifier!=\"\",region=\"us-east-1\"}",
          "legendFormat": "{{dimension_DBInstanceIdentifier}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "RDS Database Connections",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "aws_rds_database_connections_average{dimension_DBInstanceIdentifier!=\"\",region=\"us-east-1\"}",
          "legendFormat": "{{dimension_DBInstanceIdentifier}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "RDS Free Storage Space (GB)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "aws_rds_free_storage_space_average{dimension_DBInstanceIdentifier!=\"\",region=\"us-east-1\"} / 1024 / 1024 / 1024",
          "legendFormat": "{{dimension_DBInstanceIdentifier}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "gbytes",
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "green", "value": 20 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Targets Health (up)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "up",
          "legendFormat": "",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "instance": 0,
              "job": 1,
              "Value": 2
            },
            "renameByName": {
              "Value": "up"
            }
          }
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["aws", "infra", "rds", "ec2", "prometheus", "yace"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
